@model RoleViewModel
@{
    ViewData["Title"] = "Create";
}

<h1>Create Page</h1>
<br />
<br />
<form asp-controller="Role" asp-action="Create" enctype="multipart/form-data">
    <partial name="RolePartialView/_CreateEditPartialView" />
    <input type="submit" value="Create" class="btn btn-primary" />
    <a asp-controller="Role" asp-action="Index" class="btn btn-light">Back To List</a>
</form>

@section ValidationSection {
    <partial name="_ValidationScriptsPartial" />
}

